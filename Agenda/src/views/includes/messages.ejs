<% if(errors.length > 0) { %>
<div class="row">
    <div class="col my-3">
      <div class="alert-danger">
        <% errors.forEach(erro => { %>
            <%= erro %><br>
        <% }); %>

      </div>
    </div>
</div>
<% } %>


<% if(success.length > 0) { %>
    <div class="row">
        <div class="col my-3">
          <div class="alert-success">
            <% success.forEach(erro => { %>
                <%= success %><br>
            <% }); %>
    
          </div>
        </div>
    </div>
    <% } %>
    